{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bootstrap/column",
	"version": "0.1.0",
	"title": "Column",
	"category": "theme",
	"icon": "smiley",
	"description": "Bootstrap column type block.",
	"example": {},
	"attributes": {
		"xs_columns": {
			"type": "string",
			"default": "12"
		},
		"md_columns": {
			"type": "string",
			"default": ""
		},
		"xl_columns": {
			"type": "string",
			"default": ""
		},
		"xs_order": {
			"type": "string",
			"default": ""
		},
		"md_order": {
			"type": "string",
			"default": ""
		},
		"xl_order": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false
	},
	"parent": [ "bootstrap/row" ],
	"textdomain": "bootstrap",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}